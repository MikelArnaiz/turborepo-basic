{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,scss,css,yml}\"",
    "lint": "turbo run lint",
    "release": " turbo run build --filter=docs^... && changeset publish",
    "tsc": "tsc --noEmit",
    "version-packages": "changeset version",
    "version": "pnpm version-packages && pnpm i --lockfile-only",
    "pre:enter": "changeset pre enter next",
    "pre:exit": "changeset pre exit",
    "version:publish": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "prettier-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "turbo": "latest",
    "typescript": "^4.5.3"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "basic"
}
